{
    "name": "sheaxiang/shea",
    "type": "project",
    "description": "The Shea Framework.",
    "authors": [
        {
            "name": "shea",
            "email": "642290531@qq.com"
        }
    ],
    "license": "MIT",
    "keywords": ["framework", "shea", "sheaxiang"],
    "require": {
        "php": "^7.2",
        "sheaxiang/framework": "1.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {
          "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "prefer-stable": true,
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
}
